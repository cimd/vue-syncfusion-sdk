{
  "name": "@konnec/vue-syncfusion-sdk",
  "version": "0.1.0",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:unit:coverage": "vitest run --coverage.enabled=true --coverage.reporter=html",
    "test:unit:ui": "vitest --ui --coverage.enabled=true --api 9527",
    "build-only": "vite build",
    "type-check": "vue-tsc -p tsconfig.json --composite false",
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,js,cjs,mjs,vue}\"",
    "format": "prettier --write src/ test/ examples/"
  },
  "description": "An SDK for Syncfusion Vue components",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/vue-syncfusion-sdk.umd.cjs",
  "module": "./dist/vue-syncfusion-sdk.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/vue-syncfusion-sdk.js",
      "require": "./dist/vue-syncfusion-sdk.umd.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/cimd/vue-syncfusion-sdk.git"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.17.0",
    "@rushstack/eslint-patch": "^1.10.4",
    "@stylistic/eslint-plugin-js": "^2.12.1",
    "@syncfusion/ej2-vue-gantt": "28.1.35",
    "@syncfusion/ej2-vue-grids": "28.1.35",
    "@syncfusion/ej2-vue-kanban": "28.1.33",
    "@syncfusion/ej2-vue-pivotview": "28.1.33",
    "@types/jsdom": "^21.1.7",
    "@types/lodash": "^4.17.13",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/coverage-istanbul": "^2.1.8",
    "@vitest/eslint-plugin": "^1.1.20",
    "@vitest/ui": "^2.1.8",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.4",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^15.14.0",
    "jsdom": "^25.0.1",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2",
    "vite": "^6.0.5",
    "vite-plugin-dts": "^4.4.0",
    "vitest": "^2.1.8",
    "vue": "^3.5.13",
    "vue-tsc": "^2.1.10"
  },
  "keywords": [
    "Vue",
    "Syncfusion",
    "SDK"
  ],
  "author": "Christian D'Aquino",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cimd/vue-syncfusion-sdk/issues"
  }
}
